YM_NO_INLINE bool dec_to_i64(u64_t* T, const char* raw, upL_t words){
    return dec_to_i64_LittleEndian(T, raw, words);
}
YM_NO_INLINE void i64_to_dec(char* raw, const u64_t* T, upL_t words){
    i64_to_dec_Default(raw, T, words);
}
